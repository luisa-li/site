{
    "version": 2,
    "builds": [
      {
        "src": "api/*.py",
        "use": "@vercel/python"
      },
      {
        "src": "public/*",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/listening",
        "destination": "/api/index.py"
      },
      {
        "source": "/",
        "destination": "/public/index.html"
      }
    ]
  }
  